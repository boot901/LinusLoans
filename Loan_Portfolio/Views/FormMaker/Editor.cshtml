@using(@Html.BeginForm("Edit","FormMaker",FormMethod.Post)){

<div class="bar"><div class="side-bar" style="margin-left:0px;"><h3 class="centerText">Allen &amp; Overy</h3></div> <h3 class="centerText">Loan Portfolio Reports</h3>
       </div>
       <div class="underbar">
           <input value="Save" type="submit" class="save">
           <a href="@Url.Action("Index","Home")" class="canc float-right" style="background:none;">Cancel</a>
           @Html.DropDownListFor(m=>m.settings.status,new SelectList(new string[]{"Approved","Not Approved"}),new {@class="quessy"})
           <span class="float-right" style="margin-top:18px;">Report Status:</span>
           
	
       </div>
       <div class="mainerdiv2" style="background-color:IndianRed;">
       <div class="maindiv2">
		 <div class="miniwindiv">
		 	<div class="closebar">
				<span class="centerText">Form Initiation</span> <img src="Content/Arrow.png" width="50" height="50" alt="expand or close overview">
			</div>
		
			<div class="moo">
			<h1 class="below">Security</h1>
			@Html.TextBoxFor(m=>m.Security.p1,new {@Value=@Doc.Security.p1,@class="below"})

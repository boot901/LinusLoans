			<div>@Html.RadioButtonFor(m=>m.ObligationCause.NotificChngeOwnerOfBorrow.OtherVar,false,new {@style="clear:both;"})No</div>
			</div>
      		<div class="clauseform">
			@Html.TextBoxFor(m=>m.ObligationCause.NotificChngeOwnerOfBorrow.ClauseRef,"",new {@class="bigger"})
			</div>

			</li>
			<li class="liform">




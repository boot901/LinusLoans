						@for(int i=0; i<@ViewBag.lenderlist.Length;i++){
							<div class="tingy" style="margin-bottom:12px;">
							@Html.CheckBoxFor(m=>m.Common_Venders[@i],new {@id=@i, @class="checky",@Value=@Doc.Common_Venders[@i]})
							<label for="@i"><span class="checkerooo"></span>@ViewBag.lenderlist[i]</label>
							</div>
							}
						</div>
					
					</div>
					<div class="upside-form">
					 <p>Enter Aditional Lenders</p>
                     @Html.TextBoxFor(m=>m.AdditionalLenders,new {@class="bigger",@style="float:left",@Value=@Doc.AdditionalLenders})
                  
					</div>
				<div class="upside-form" style="">
					 <p>Final Report Distribution List</p>
                  				   @Html.TextBoxFor(m=>m.final_report_dist_list,new {@class="bigger",@Value=@Doc.final_report_dist_list})

					</div>
				
                   </div>
			<div  style="height:250px;" class="sec">
				 <div class="sideline">1.3 </div>
				<div class="upside-form">
					 <p>Principal sponsors (if any)</p>
                    				@Html.TextBoxFor(m=>m.PSponsor,new {@class="bigger",@Value=@Doc.PSponsor})	
                    
					</div>
				</div>
			<div class="sec" style="height:250px;">
			<div class="sideline">1.4 </div>
				<div class="upside-form">
					 <p>Original Date of making</p>
									
                    				@Html.TextBoxFor(m=>m.OrigDate,new {@class="bigger",@Value=@Doc.OrigDate})

                    
					</div>
				</div>
			<div class="sec" style="height:250px;">
				<div class="sideline">1.5 </div>
				<div class="upside-form">
					 <p>Date of most recent amendment / amendment and restatement (if different)</p>
									
                    				@Html.TextBoxFor(m=>m.Date,new {@class="bigger",@Value=@DateTime.Now.ToShortDateString()})

                    
					
				</div>
			</div>

			<div ng-app="App">
			<div class="sec2" id="tbl1">
				<div class="sideline">1.6 </div>
				<div>
					 <p style="margin-left:3%">Total Commitments</p>
						<div ng-controller="TableController">			
                    	<table id="commitments" style="width=100%;">
						<tbody >
                    	<tr>
						<th>Facility</th>
						<th>Currency</th>
						<th>Amount</th>	
						</tr>
					
						<tr class="lol" ng-repeat="n in range(num) track by $index">
							
							<td> 
								<a ng-click="RemoveRow()" style="background-color:IndianRed;position:absolute;">X</a>
								<select value="Commit_{{$index}}__Facility" id="Commit_{{$index}}__Facility" name="Commit[{{$index}}].Facility" style=""><option value="ko">ko</option>
								<option value="lol">lol</option>
								</select>	
							</td>
							<td>
								<select value="Commit_{{$index}}__Facility"  id="Commit_{{$index}}__Currency" name="Commit[{{$index}}].Currency" style=""><option value="ko">ko</option>
								<option value="lol">lol</option>
								</select>	
							</td>					
							<td>
								<input type="number" name="Commit[{{$index}}].Amount" id="Commit_{{$index}}__Amount">
							</td>

						</tr>

						</tbody>
						</table>
						<button type="button" ng-click="AddRow()" class="save" style="width:30%;float:left;margin-left:35%;">Add commitement</button>
						</div>

				</div>
			</div>
				
				<div class="sec2" id="tbl2">
				<div class="sideline">1.7 </div>
				<div class="">
									
					<div>
					 <p style="margin-left:3%">Contacts</p>
						<div ng-controller="TableController2">				
                    	<table id="contacts" style="width=100%;">
						<tbody >
                    	<tr>
						<th>Full Name</th>
						<th>Role</th>
						<th>Contact Number</th>	
						</tr>
						<tr class="lol" ng-repeat="n in range(num) track by $index">
			
							<td> 
								<a ng-click="RemoveRow()" style="background-color:IndianRed;">X</a>
								<input type="text" style="height:30px;" name="Contacts[{{$index}}].FullName" id="Contacts_{{$index}}__FullName">
							</td>
								<td>
								<input type="text" style="height:30px;" name="Contacts[{{$index}}].Role" id="Contacts_{{$index}}__Role">
									
								</td>					
							<td>
								<input type="text" style="height:30px;" name="Contacts[{{$index}}].Contact" id="Contacts_{{$index}}__Contact">
							</td >

						</tr>

						</tbody>
						</table>
							<button type="button" ng-click="AddRow()" class="save" style="width:30%;float:left;margin-left:35%;">Add contact</button>
						</div>
						</div>
						

                    
					
				</div>
			</div>
			</div>
				<div class="sec" style="height:250px;">
						<div class="sideline">1.8</div>
						<div class="upside-form">
						<p style="margin-left:3%">Is this file highly restricted<br> or matter price sensitive?</p>
						<div>@Html.RadioButtonFor(m=>m.restricted,true, new {@Checked=""})Yes</div>
						@if(Doc.restricted==false){
						<div>@Html.RadioButtonFor(m=>m.restricted,false,new {@Checked=""})No</div>}
						@if(Doc.restricted==true){
						<div>@Html.RadioButtonFor(m=>m.restricted,false)No</div>}


						</div>
					</div>
                   </div>
				
               </div>
				<div class="miniwindiv" style="margin-bottom:2%;">
					<div class="closebar" style="height:50px;padding-bottom:25px;">
					<span class="centerText">Facilities</span><img src="Content/Arrow.png" width="50" height="50" alt="expand or close overview"> <span style="float:right; margin-top:30px;">Clause Ref</span>
				</div>
				<div class="moo" style="height:100%;">
				<div class="sec" style="height:250px;">
				<div class="sideline">2.1</div>
				<table>
					<tbody>

						<tr>
							<th>Facility</th>
							<th>Type</th>
							<th>Maturity</th>
							<th>Currency</th>
							<th>Amount</th>
						</tr>
						<tr class="lol">
							<td>@Html.TextBoxFor(m=>m.Facilities.Facility)</td>
							<td>@Html.TextBoxFor(m=>m.Facilities.Type)</td>
							<td>@Html.TextBoxFor(m=>m.Facilities.Maturity)</td>
							<td>@Html.DropDownListFor(m=>m.Facilities.Currency, new SelectList(new string[] {"DDD","AAA"}))</td>
							<td><input type="number" name="Facilities.Amount" id="Facilities_Amount"></td>
						</tr>
					</tbody>
				</table>
				</div>

		<div  style="height:250px;" class="sec">
		 <div class="sideline">2.2 </div>
			<p style="margin-left:3%;">Purpose Clause</p>
			<div class="upside-form">
       		@Html.TextBoxFor(m=>m.PurposeClause.OtherVar,new {@class="bigger", @Value=@Doc.PurposeClause.OtherVar})	
      		</div>
      		<div class="clauseform">
			@Html.TextBoxFor(m=>m.PurposeClause.ClauseRef,"",new {@class="bigger", @Value=@Doc.PurposeClause.ClauseRef})
			</div>	            
		</div>

		<div  style="height:250px;" class="sec">
				 <div class="sideline">2.3 </div>
					<p style="margin-left:3%;">What level of consent is required to approve an extension of maturity</p>
				<div class="upside-form">
					 
       @Html.DropDownListFor(m=>m.consentlvl,new SelectList(new string[]{"my dad","my mom"}),new {@class="bigger"})	

					</div>
		</div>

		<div  style="height:250px;" class="sec">
				 <div class="sideline">2.4 </div>
					<p style="margin-left:3%;">Extension options</p>
				<div class="upside-form">
					 
       @Html.DropDownListFor(m=>m.extension,new SelectList(new string[]{"my dad","my mom"}),new {@class="bigger"})	

					</div>
		</div>
		<div  style="height:250px;" class="sec">
		 <div class="sideline">2.5 </div>
			<p style="margin-left:3%;">Does the credit agreement contain interest rate floors(e.g. LIBOR may not be below zero)?</p>
			@Html.DefaultClauseRadioListFor(m=>m.CreditToInterestRate.OtherVar,m=>m.CreditToInterestRate.ClauseRef,new string[]{"Yes","No"},new bool[]{true,false},Doc.CreditToInterestRate.ClauseRef,Doc.CreditToInterestRate.OtherVar)

		
      		
				

			</div>    


		<div  style="height:250px;" class="sec">

						
		 <div class="sideline">2.6 </div>
			<p style="margin-left:3%;">Is disposal a mandatory prepayment event?</p>
			@Html.DefaultClauseRadioListFor(m=>m.DisposeMandatorPrepayment.OtherVar,m=>m.DisposeMandatorPrepayment.ClauseRef,new string[]{"Yes","No"},new bool[]{true,false},Doc.DisposeMandatorPrepayment.ClauseRef,Doc.DisposeMandatorPrepayment.OtherVar)
			</div>	            
		<div  style="height:250px;" class="sec">

							
		 <div class="sideline">2.7 </div>
			<p style="margin-left:3%;">Is a change of control a mandatory prepayment event?</p>

			@Html.DefaultClauseRadioListFor(m=>m.ChangeMandatorPrepayment.OtherVar,m=>m.ChangeMandatorPrepayment.ClauseRef,new string[]{"Yes","No"},new bool[]{true,false},Doc.ChangeMandatorPrepayment.ClauseRef,Doc.ChangeMandatorPrepayment.OtherVar)

      	</div>	
		<div  style="height:250px;" class="sec">					
		<div class="sideline">2.8 </div>
			<p style="margin-left:3%;">Purpose Clause</p>
			@Html.DefaultClauseRadioListFor(m=>m.CreditToTax_Gross_up.OtherVar,m=>m.CreditToTax_Gross_up.ClauseRef,new string[]{"Yes","No"},new bool[]{true,false},Doc.CreditToTax_Gross_up.ClauseRef,Doc.CreditToTax_Gross_up.OtherVar)
			</div>
				<div  style="height:250px;" class="sec">
		<div class="sideline">2.9 </div>
			<p style="margin-left:3%;">Does the credit agreement contain a negative pledge?</p>
			@Html.DefaultClauseRadioListFor(m=>m.CreditToNegPledge.OtherVar,m=>m.CreditToNegPledge.ClauseRef,new string[]{"Yes","No"},new bool[]{true,false},Doc.CreditToNegPledge.ClauseRef,Doc.CreditToNegPledge.OtherVar)
			</div>

				
		<div  style="height:250px;" class="sec">
		<div class="sideline">2.10 </div>
			<p style="margin-left:3%;">Does the credit agreement contain a restriction <br> on incurring financial indebtedness?</p>
						@Html.DefaultClauseRadioListFor(m=>m.CreditToRestriction_IncurrringIndebtedness.OtherVar,m=>m.CreditToRestriction_IncurrringIndebtedness.ClauseRef,new string[]{"Yes","No"},new bool[]{true,false},Doc.CreditToRestriction_IncurrringIndebtedness.ClauseRef,Doc.CreditToRestriction_IncurrringIndebtedness.OtherVar)
		</div>
		<div  style="vertical-align:top; height:500px;" class="sec">
		<div class="sideline">2.11 </div>
			<p style="margin-left:3%;">Does the credit agreement contain the following financiall covenants</p>

			<ol type="a" style="">
			<li class="liform">

			<div style="width:50%;position:relative; margin-left:0px; margin-top:12px;">
			<span style="">debt:EBITDA<br>(leverage)</span> 
			@Html.DefaultNoUpsideClauseRadioListFor(m=>m.FinancialCovenants.debtLeverage.OtherVar,m=>m.FinancialCovenants.debtLeverage.ClauseRef,YesNo,new bool[]{true,false},Doc.FinancialCovenants.debtLeverage.ClauseRef,Doc.FinancialCovenants.debtLeverage.OtherVar)
			</div>
				</li>
			<li class="liform">

			<div style="width:50%;position:relative; margin-left:0px; margin-top:12px;">
			<span style="">EBITDA:interest<br>(interest cover)?</span> 
			@Html.DefaultNoUpsideClauseRadioListFor(m=>m.FinancialCovenants.interestCover.OtherVar,m=>m.FinancialCovenants.interestCover.ClauseRef,YesNo,new bool[]{true,false},Doc.FinancialCovenants.interestCover.ClauseRef,Doc.FinancialCovenants.interestCover.OtherVar)
			</div>

			</li>
			<li class="liform">



			<div style="width:50%;position:relative; margin-left:0px; margin-top:12px;">
			<span style="">Cashflow:debt<br>service costs<br>(cashflow cover)</span> 
			@Html.DefaultNoUpsideClauseRadioListFor(m=>m.FinancialCovenants.CashflowCover.OtherVar,m=>m.FinancialCovenants.CashflowCover.ClauseRef,YesNo,new bool[]{true,false},Doc.FinancialCovenants.CashflowCover.ClauseRef,Doc.FinancialCovenants.CashflowCover.OtherVar)
			</div>

			</li>
			<li class="liform">



			<div style="width:50%;position:relative; margin-left:0px; margin-top:12px;">
			<span style="">tangible net worth?<br>(leverage)</span> 
			@Html.DefaultNoUpsideClauseRadioListFor(m=>m.FinancialCovenants.tangibleNetWorth.OtherVar,m=>m.FinancialCovenants.tangibleNetWorth.ClauseRef,YesNo,new bool[]{true,false},Doc.FinancialCovenants.tangibleNetWorth.ClauseRef,Doc.FinancialCovenants.tangibleNetWorth.OtherVar)

			</div>

			</li>
			</ol>
		</div>
			<div  style="height:250px;" class="sec">					
		<div class="sideline">2.12 </div>
			<p style="margin-left:3%;">Does the credit agreement <br> contain any other financial covenants</p>

			@Html.DefaultClauseRadioListFor(m=>m.MoreFinancialCovenants.OtherVar,m=>m.MoreFinancialCovenants.ClauseRef,YesNo,new bool[]{true,false},Doc.MoreFinancialCovenants.ClauseRef,Doc.MoreFinancialCovenants.OtherVar)

			</div>

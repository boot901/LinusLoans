			<span style="">Notification of any default?</span> 
			<div>@Html.RadioButtonFor(m=>m.ObligationCause.NotificAnyDefault.OtherVar,true,new {@style="clear:both;"})Yes</div>
			<div>@Html.RadioButtonFor(m=>m.ObligationCause.NotificAnyDefault.OtherVar,false,new {@style="clear:both;"})No</div>
			</div>
      		<div class="clauseform">
			@Html.TextBoxFor(m=>m.ObligationCause.NotificAnyDefault.ClauseRef,"",new {@class="bigger"})
			</div>

			</li>

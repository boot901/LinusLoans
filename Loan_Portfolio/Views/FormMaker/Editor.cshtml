			<div>@Html.RadioButtonFor(m=>m.ObligationCause.FurtherInfoRequestFinConBusOp.OtherVar,false,new {@style="clear:both;"})No</div>
			</div>
      		<div class="clauseform">
			@Html.TextBoxFor(m=>m.PurposeClause.ClauseRef,"",new {@class="bigger"})
			</div>

			</li>
						<li class="liform">




			@Html.TextBoxFor(m=>m.Security.p3,new {@class="below",@Value=@Doc.Security.p3})
			@Html.TextBoxFor(m=>m.Security.p4,new {@class="below",@Value=@Doc.Security.p4})
			@Html.TextBoxFor(m=>m.Security.p5,new {@class="below",@Value=@Doc.Security.p5})
			<h1 class="below">Text Description*</h1>
			@Html.TextAreaFor(m=>m.Security.Comment,new {@class="textar",@Value=@Doc.Security.Comment})
			</div>
			</div>

		
           <div class="miniwindiv">
               <div class="closebar">
                   <span class="centerText">Overview</span> <img src="Content/Arrow.png" width="50" height="50" alt="expand or close overview" onclick="">
			
                       </div>
		
               <div class="moo" style="">
                   <div class="sideline">1.1 Form Identification</div>
                   
                   <div style="height:250px;" class="sec">
                       
                       <div class="upside-form">
                           <p>Allen &amp; Overy Client/Matter Number</p>
                           @Html.TextBoxFor(m=>m.FormIdentification.AllenOveryCorM_Num,new {@class="bigger",@Value=@Doc.FormIdentification.AllenOveryCorM_Num})
                               </div>
                       <div class="upside-form">
                           <p>Principal borrowers</p>
                          	@Html.TextBoxFor(m=>m.FormIdentification.Principle_Borrow,new {@class="bigger",@Value=@Doc.FormIdentification.Principle_Borrow})
                               </div>
                       <div class="upside-form">
                           <p>Relevant facility e.g. Senior,Junior etc.</p>
                         	@Html.TextBoxFor(m=>m.FormIdentification.Relevant_Facility,new {@class="bigger",@Value=@Doc.FormIdentification.Relevant_Facility})
                               </div>
                   </div>
                  
                   
                   <div class="sec" style="height:400px;">
					 <div class="sideline">1.2 Initial lenders in the syndicate</div>
						<div style="height:100%;" class="upside-form">


@using System.Web.Mvc;
@using System.Text;
@functions{
public static string YesNo(bool e){
	if(e==true){
		return "Yes";
	}else{
		return "No";
	}
}
	
}

@model Loan_Portfolio.Models.DocDetail
@{
Loan_Portfolio.Models.DocDetail Doc= (Loan_Portfolio.Models.DocDetail)ViewData["DocDetail"];
}

<div class="bar"><div class="side-bar" style="margin-left:0px;"><h3 class="centerText">Allen &amp; Overy</h3></div> <h3 class="centerText">Loan Portfolio Reports</h3>
       </div>
       <div class="underbar">
           <a href="@Url.Action("Index","Home")"><input value="Go Back" type="button"  class="save"></a>
           <h3 class="quessy">@Doc.settings.status</h3>
           <span class="float-right" style="margin-top:18px;">Report Status:</span>
           
	
       </div>
       <div class="mainerdiv2" style="background-color:IndianRed;">
       <div class="maindiv2">
		 <div class="miniwindiv">
		 	<div class="closebar">
				<span class="centerText">Form Initiation</span> <img src=@Href("~/Content/Arrow.png") width="50" height="50" alt="expand or close overview">
			</div>
			
			<div class="moo">
			<h1 class="below">Security</h1>
			<h3 class="below">@Doc.Security.p1</h3>
			<h3 class="below">@Doc.Security.p2</h3>
			<h3 class="below">@Doc.Security.p3</h3>
			<h3 class="below">@Doc.Security.p4</h3>
			<h3 class="below">@Doc.Security.p5</h3>

			<h1 class="below">Text Description*</h1>
			<p class="textar below">@Doc.Security.Comment</p>
			</div>
			</div>

		
           <div class="miniwindiv">
               <div class="closebar">
                   <span class="centerText">Overview</span> <img src=@Href("~/Content/Arrow.png") width="50" height="50" alt="expand or close overview" onclick="">
			
                       </div>
		
               <div class="moo" style="">
                   <div class="sideline">1.1 Form Identification</div>
                   
                   <div style="height:250px;" class="sec">
                       
                       <div class="upside-form">
                           <p>Allen &amp; Overy Client/Matter Number</p>
                           <h3 class="bigger">@Doc.FormIdentification.AllenOveryCorM_Num</h3>
                               </div>
                       <div class="upside-form">
                           <p>Principal borrowers</p>
                           <h3 class="bigger">@Doc.FormIdentification.Principle_Borrow</h3>
                               </div>
                       <div class="upside-form">
                           <p>Relevant facility e.g. Senior,Junior etc.</p>
 					<h3 class="bigger">@Doc.FormIdentification.Relevant_Facility</h3>
					</div>
                   </div>
                  
                   
                   <div class="sec" style="height:400px;">
					 <div class="sideline">1.2 Initial lenders in the syndicate</div>
						<div style="height:100%;" class="upside-form">

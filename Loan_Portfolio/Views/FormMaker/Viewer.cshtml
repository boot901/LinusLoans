						<div style="background:white;overflow-x:hidden;overflow-y:scroll;float:left; height:129px;border:grey solid 1px; margin-bottom:10%; width:30%;">

						@for(int i=0; i<@ViewBag.lenderlist.Length;i++){
							if(@Doc.Common_Venders[i]==true){
							<div class="tingy" style="margin-bottom:12px;">
							<h9 class="checky">@ViewBag.lenderlist[i]</h9>
							</div>
							}
							}
						</div>
					
					</div>
					<div class="upside-form">
					 <p>Enter Aditional Lenders</p>
                      <h3 class="bigger">@Doc.AdditionalLenders</h3>
                  
					</div>
				<div class="upside-form" style="">
					 <p>Final Report Distribution List</p>
						<h3 class="bigger">@Doc.final_report_dist_list</h3>
					</div>
				
                   </div>
			<div  style="height:250px;" class="sec">
				 <div class="sideline">1.3 </div>
				<div class="upside-form">
					 <p>Principal sponsors (if any)</p>
                    				<h3 class="bigger">@Doc.PSponsor</h3>	
                    
					</div>
				</div>
			<div class="sec" style="height:250px;">
			<div class="sideline">1.4 </div>
				<div class="upside-form">
					 <p>Original Date of making</p>
									
						<h3 class="bigger">@Doc.OrigDate</h3>
                    
					</div>
				</div>
			<div class="sec" style="height:250px;">
				<div class="sideline">1.5 </div>
				<div class="upside-form">
					 <p>Date of most recent amendment / amendment and restatement (if different)</p>
									
						<h3 class="bigger">@Doc.Date</h3>
                    
					
				</div>
			</div>

			<div ng-app="App">
			<div class="sec2" id="tbl1">
				<div class="sideline">1.6 </div>
				<div>
					 <p style="margin-left:3%">Total Commitments</p>
								
                    	<table id="commitments" style="width=100%;">
						<tbody >
                    	<tr>
						<th>Facility</th>
						<th>Currency</th>
						<th>Amount</th>	
						</tr>
					@foreach(var i in @Doc.Commit){
						<tr class="lol">
							
							<td> 
								<h3 class="bigger">@i.Facility</h3>
							</td>
							<td>
								<h3 class="bigger">@i.Currency</h3>	
							</td>					
							<td>
								<h3 class="bigger">@i.Amount</h3>
							</td>

						</tr>
						}
						</tbody>
						</table>



				</div>
			</div>
				
				<div class="sec2" id="tbl2">
				<div class="sideline">1.7 </div>
				<div class="">
									
					<div>
					 <p style="margin-left:3%">Contacts</p>
									
                    	<table id="contacts" style="width=100%;">
						<tbody >
                    	<tr>
						<th>Full Name</th>
						<th>Role</th>
						<th>Contact Number</th>	
						</tr>
							@foreach(var i in @Doc.Contacts){
						<tr class="lol">
							
							<td> 
								<h3 class="bigger">@i.FullName</h3>
							</td>
							<td>
								<h3 class="bigger">@i.Role</h3>	
							</td>					
							<td>
								<h3 class="bigger">@i.Contact</h3>
							</td>

						</tr>
						}

						</tbody>
						</table>

						</div>
						

                    
					
				</div>
			</div>
			</div>
				<div class="sec" style="height:250px;">
						<div class="sideline">1.8</div>
						<div class="upside-form">
						<p style="margin-left:3%">Is this file highly restricted<br> or matter price sensitive?</p>
						 <h3 class="bigger">@YesNo(Doc.restricted)</h3>
						</div>
					</div>
                   </div>
				
               </div>
				<div class="miniwindiv" style="margin-bottom:2%;">
					<div class="closebar" style="height:50px;padding-bottom:25px;">
					<span class="centerText">Facilities</span><img src=@Href("~/Content/Arrow.png") width="50" height="50" alt="expand or close overview"> <span style="float:right; margin-top:30px;">Clause Ref</span>
				</div>
				<div class="moo" style="height:100%;">
				<div class="sec" style="height:250px;">
				<div class="sideline">2.1</div>
				<table>
					<tbody>

						<tr>
							<th>Facility</th>
							<th>Type</th>
							<th>Maturity</th>
							<th>Currency</th>
							<th>Amount</th>
						</tr>
						<tr class="lol">
							<td><h3 class="bigger">@Doc.Facilities.Facility</h3></td>
							<td><h3 class="bigger">@Doc.Facilities.Type</h3></td>
							<td><h3 class="bigger">@Doc.Facilities.Maturity</h3></td>
							<td><h3 class="bigger">@Doc.Facilities.Currency</h3></td>
							<td><h3 class="bigger">@Doc.Facilities.Amount"</h3></td>
						</tr>
					</tbody>
				</table>
				</div>

		<div  style="height:250px;" class="sec">
		 <div class="sideline">2.2 </div>
			<p style="margin-left:3%;">Purpose Clause</p>
			<div class="upside-form">
				<h3 class="bigger">@Doc.PurposeClause.OtherVar</h3>   
			</div>
      		<div class="clauseform">
			<h3 class="bigger">@Doc.PurposeClause.ClauseRef</h3>
			</div>	            
		</div>

		<div  style="height:250px;" class="sec">
